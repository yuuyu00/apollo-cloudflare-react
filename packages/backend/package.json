{
  "name": "@apollo-cloudflare-react/backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/server",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run generate && tsc",
    "prisma-migrate": "prisma migrate dev --name",
    "prisma-deploy": "prisma migrate deploy",
    "generate": "pnpm run generate:prisma && pnpm run generate:codegen",
    "generate:prisma": "prisma generate",
    "generate:codegen": "pnpm run generate:codegen:prepare && pnpm run generate:codegen:concat && pnpm run generate:codegen:dts",
    "generate:codegen:prepare": "rimraf schema/schema.gql",
    "generate:codegen:concat": "concat -o schema/schema.gql schema/*.gql",
    "generate:codegen:dts": "graphql-codegen --config codegen.yml",
    "insert-seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seeds.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "typescript": "^5.8.3",
    "@graphql-codegen/cli": "^5.0.7",
    "prisma": "^6.9.0",
    "@prisma/client": "^6.9.0",
    "apollo-server": "^3.13.0",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "@types/node": "15.3.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "npm-run-all": "4.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "concat": "1.0.3",
    "rimraf": "3.0.2"
  },
  "volta": {
    "node": "22.11.0"
  }
}
