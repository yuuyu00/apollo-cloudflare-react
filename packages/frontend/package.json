{
  "name": "@apollo-cloudflare-react/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'pnpm codegen-watch'",
    "dev:worker": "wrangler dev --port 3001",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate": "graphql-codegen",
    "codegen-watch": "graphql-codegen --watch",
    "type-check": "tsc --noEmit",
    "types": "wrangler types",
    "clean": "rm -rf dist",
    "deploy:dev": "pnpm run clean && vite build --mode staging && wrangler deploy --env dev",
    "deploy:prod": "pnpm run clean && vite build --mode production && wrangler deploy --env prod"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@clerk/clerk-react": "^5.45.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.18.1",
    "graphql": "^16.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.0.2",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-url-parser": "^1.1.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  },
  "volta": {
    "node": "22.11.0"
  }
}
